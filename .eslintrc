{
    "env": {
      "browser": true,
      "jquery": true
    },
    "extends": "eslint:recommended",
    "globals": {
      "pokemons": true
    },
    "rules": {
      "indent": ["error", 2],
      "quotes": ["error", "single"],
      "semi": ["error", "always"],
      "no-console": "off"
    }
}
